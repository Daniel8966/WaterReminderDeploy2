              <div class="row">
                <div class="col-sm-3">
                  <h6 class="mb-0">Peso</h6>
                </div>
                <div class="col-sm-9 text-secondary">
                  <%=peso%> kg
                </div>
              </div>
              <hr>
              <div class="row">
                <div class="col-sm-3">
                  <h6 class="mb-0">Hora de dormir</h6>
                </div>
                <div class="col-sm-9 text-secondary">
                  <%=horaDormir%> .
                </div>
              </div>
              <hr>
              <div class="row">
                <div class="col-sm-3">
                  <h6 class="mb-0">Hora de despertar</h6>
                </div>
                <div class="col-sm-9 text-secondary">
