  <script>
    // Obtener todas las tablas con la clase "tabla"
    const tablas = document.querySelectorAll('table');

    // Iterar sobre cada tabla
    tablas.forEach(tabla => {
      // Obtener todos los elementos con la clase "valor" dentro de la tabla
      const elementos = tabla.querySelectorAll('.valor');

      // Variables para almacenar los valores de cada tabla
      let gramos, calorias;

      // Iterar sobre cada elemento y obtener los datos necesarios
      elementos.forEach(elemento => {
        // Obtener el ID del elemento para determinar el tipo de dato
        const id = elemento.getAttribute('id');
        const valor = elemento.textContent;

        // Extraer los valores numéricos de los datos según el ID
        if (id) {
          gramos = parseFloat(valor);
        } else if (id) {
          calorias = parseInt(valor);
        } 
      });

      // Crear elementos de input ocultos para enviar los datos
      const gramosInput = document.createElement('input');
      gramosInput.setAttribute('type', 'hidden');
      gramosInput.setAttribute('name', 'azucar');
      gramosInput.setAttribute('value', gramos || '');

      const caloriasInput = document.createElement('input');
      caloriasInput.setAttribute('type', 'hidden');
      caloriasInput.setAttribute('name', 'calorias');
      caloriasInput.setAttribute('value', calorias || '');


      // Agregar los inputs ocultos a la tabla
      tabla.appendChild(gramosInput);
      tabla.appendChild(caloriasInput);
      

    });
  </script>



